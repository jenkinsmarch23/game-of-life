pipeline {
         agent { label 'mvn'}
         stages {
                stage('build') {
                       steps {
                              git url: 'https://github.com/wakaleo/game-of-life.git',
                                  branch: 'master'
                    
                             }
                       } 
                      stage('javapath') {
                       steps {
                             sh '''
                                   export PATH="/usr/lib/jvm/java-1.8.0-openjdk-amd64/bin:$PATH"
                                   java -version        
                                '''
                             }
                      } 
                stage('sonar'){
                       steps{
                             withSonarQubeEnv ('sonarabhi'){
                             sh 'mvn clean package sonar: sonarabhi'   
                             }
                            }
                     }     
                
                }
         }

